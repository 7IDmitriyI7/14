using System;

namespace _14._5
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите А");
            int a = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Введите B");
            int b = Convert.ToInt32(Console.ReadLine());

            while ((a != 0) && (b != 0))
            {
                if (a > b)
                    a -= b;
                else
                    b -= a;
            }
            Console.WriteLine("НОД "+a);
        }
    }
}
