using System;

namespace _14._6
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine("Введите число фибоначи");
            int n = Convert.ToInt32(Console.ReadLine());
            if (n > 1)
            {
                int f1 = 1, f2 = 1, f = 0, k = 3;
                while (f < n)
                {
                    ++k;
                    f = f2 + f1;
                    f2 = f1;
                    f1 = f;
                }
                Console.WriteLine(k);
            }
            else { Console.WriteLine("Ошибка ввода"); }
        }
    }
}
